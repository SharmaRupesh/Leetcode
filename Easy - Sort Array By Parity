class Solution {
    public int[] sortArrayByParity(int[] A) {
        int result[] = new int[A.length];
        int first=0,last=result.length-1;
        for(int i:A)
        {
            if(i%2==0)
                result[first++]=i;
            else
                result[last--] =i; 
        }
        return result;
    }
}
